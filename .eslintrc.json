{
    "env": {
        "commonjs": true,
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "max-len": [1, {
            "ignoreComments": true,
            "ignoreStrings": true
        }]
    }
}
